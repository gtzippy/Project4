<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/mydb</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password"/>
    <property name="hibernate.show_sql">true</property>
    <property name="hibernate.current_session_context_class">thread</property>
    <property name="hibernate.query.factory_class">org.hibernate.hql.internal.classic.ClassicQueryTranslatorFactory</property>
    <mapping resource="edu/gatech/sad/project4/entities/Processingstatustable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Offeredcoursestable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Studentcourseassignmenttable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Professorstable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Coursetable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Professorcourseassignmenttable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Studentpreferencestable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Tacourseassignmenttable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Studenttable.hbm.xml"/>
    <mapping resource="edu/gatech/sad/project4/entities/Administratortable.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
